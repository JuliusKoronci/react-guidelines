{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "cliOptions": {
    "exclude": [
      "**/*.story.tsx"
    ]
  },
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "lowercase-first"
      }
    ]
  }
}

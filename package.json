{
  "name": "react-ts-guidelines",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "prettier --single-quote --write --trailing-comma all",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "server:dev": "nodemon server/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "lint-staged": "^7.0.2",
    "loadable-components": "^1.2.0",
    "prettier": "^1.11.1",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "2.14.0",
    "react-spinners": "^0.2.6",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.5",
    "styled-normalize": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addon-notes": "^3.4.0",
    "@storybook/addon-viewport": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "@types/ramda": "^0.25.21",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-loadable": "^5.3.3",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/react-transition-group": "^2.0.7",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.43",
    "@types/redux-devtools-dock-monitor": "^1.1.32",
    "@types/redux-devtools-log-monitor": "^1.0.33",
    "@types/redux-logger": "^3.0.5",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "^1.13.5",
    "ajv": "^6.4.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-runtime": "^7.0.0-beta.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "react-test-renderer": "^16.3.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts",
      "!src/index.tsx",
      "!src/**/styled.ts",
      "!src/app/configureStore.ts",
      "!src/app/rootReducer.ts",
      "!src/registerServiceWorker.ts",
      "!src/setupTests.ts",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public/",
      "!<rootDir>/src/theme/*",
      "!<rootDir>/src/**/@types/*",
      "!<rootDir>/server/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
